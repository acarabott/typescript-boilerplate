{
  "files": [
    "./src/app.ts",
  ],
  "compilerOptions": {
    "outDir": "./dist",
    "sourceMap": false,
    "module": "es6",
    "target": "es2017",
    "lib": [
      "dom",
      "es6",
      "es2015",
      "es2015.collection",
      "es2015.iterable",
      "es2015.symbol",
      "es2017",
      "es2017.object"
    ],
    "allowJs": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "downlevelIteration": true
  }
}
